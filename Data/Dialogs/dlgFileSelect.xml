<?xml version="1.0"?>

<Form Name="frmFileSelect" Width="320" Height="240" Caption="File browser">
  <Button Name="cmdClose" Caption="Close" X="2" Y="0" Width="56" Height="28" OnClick="OnFileCloseClicked"/>
  
  <Edit Name="prpFltType" Caption="File type" X="60" Y="0" Width="190" Height="28" CaptionWidth="70">
    <DataField DataType="enum"/>
  </Edit>
  
  <Button Name="cmdRefresh" Caption="Refresh" X="252" Y="0" Width="66" Height="28" OnClick="OnRefreshClicked"/>
  
  <Edit Name="prpStorageFree" Caption="Free" X="2" Width="98" Height="36" CaptionWidth="0" ReadOnly="1">
    <DataField DataType="double" DisplayFormat="%.0f kb" EditFormat="%.0f" ReadOnly="1"/>
  </Edit>
  
  <List Name="frmFileList" X="102" Y="38" Width="-1" Height="-1" ItemHeight="18"/>
</Form>
